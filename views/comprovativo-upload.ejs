                <span>Clique aqui ou arraste a foto</span>
                <div>
                  <input
                    required
                    accept=".jpg, .jpeg, .png"
                    type="file"
                    class="custom-file-input"
                    id="customFile"
                  />
                </div>
                <p>Nenhum arquivo selecionado</p>
              </div>
              <input type="submit" value="Enviar">
            </div>
          </form>
        </div>
